}
image.data-v-b5b6feed {
  width: 100%;
  height: 100%;
}
.adminManage.data-v-b5b6feed {
  padding: 24rpx;
  height: 100vh;
  overflow: hidden;
  background-color: #f7f7f7;
}
.adminManage .content.data-v-b5b6feed {
  border-radius: 24rpx;
  background-color: #fff;
}
.adminManage .content .item.data-v-b5b6feed {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 24rpx;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.adminManage .content .item.data-v-b5b6feed:nth-last-child(1) {
  border: none;
}
.adminManage .content .item .left.data-v-b5b6feed {
  display: flex;
  align-items: center;
}
.adminManage .content .item .left .box.data-v-b5b6feed {
  margin-right: 16rpx;
  height: 24rpx;
  width: 8rpx;
  background-color: #399bfe;
